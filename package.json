  "name": "@gbernard/template-htmx",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "by-node-env",
    "build:development": "parcel watch --no-autoinstall --no-source-maps",
    "build:production": "parcel build --no-autoinstall --no-source-maps",
    "clean": "rimraf .parcel-cache dist",
    "lint": "eslint --max-warnings=0 server.ts src/script.ts",
    "postbuild": "rimraf .parcel-cache",
    "prebuild": "rimraf .parcel-cache dist",
    "start": "by-node-env",
    "start:development": "DEBUG=* tsx watch --clear-screen=false server.ts",
    "start:production": "tsx server.ts",
    "version": "echo $npm_package_version"
  },
  "source": "src/index.pug",
  "type": "module",
  "version": "0.0.1"
